# 문제 링크 : https://school.programmers.co.kr/learn/courses/30/lessons/301650

SELECT
    THRID_GEN.ID AS 'ID'
FROM
    (SELECT ID FROM ECOLI_DATA
    WHERE PARENT_ID IS NULL) FIRST_GEN
    INNER JOIN (SELECT ID, PARENT_ID FROM ECOLI_DATA) SECOND_GEN
    ON FIRST_GEN.ID = SECOND_GEN.PARENT_ID
    INNER JOIN (SELECT ID, PARENT_ID FROM ECOLI_DATA) THRID_GEN
    ON SECOND_GEN.ID = THRID_GEN.PARENT_ID
ORDER BY 1;
