# 문제 링크 : https://school.programmers.co.kr/learn/courses/30/lessons/298519

SELECT
   TYPE.FISH_COUNT AS 'FISH_COUNT',
   TYPE.MAX_LENGTH AS 'MAX_LENGTH',
   TYPE.FISH_TYPE AS 'FISH_TYPE'
FROM
    (SELECT
        MAX(IFNULL(LENGTH, 10)) AS 'MAX_LENGTH',
        COUNT(ID) AS 'FISH_COUNT',
        FISH_TYPE
    FROM FISH_INFO
    GROUP BY FISH_TYPE
    HAVING SUM(IFNULL(LENGTH,10))/COUNT(ID)>=33) TYPE
ORDER BY 3;
