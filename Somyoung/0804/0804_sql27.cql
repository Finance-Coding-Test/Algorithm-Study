# 문제 링크 : https://school.programmers.co.kr/learn/courses/30/lessons/276035

SELECT
    DISTINCT(DEV.ID) AS 'ID',
    DEV.EMAIL AS 'EMAIL',
    DEV.FIRST_NAME AS 'FIRST_NAME',
    DEV.LAST_NAME AS 'LAST_NAME'
FROM
    (SELECT CODE FROM SKILLCODES WHERE CATEGORY = 'Front End') SKILL
    INNER JOIN DEVELOPERS DEV
    ON (DEV.SKILL_CODE & SKILL.CODE)
ORDER BY 1;
