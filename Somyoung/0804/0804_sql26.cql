# 문제 링크 : https://school.programmers.co.kr/learn/courses/30/lessons/59411

SELECT
    OUTS.ANIMAL_ID AS 'ANIMAL_ID',
    OUTS.NAME AS 'NAME'
FROM
    (SELECT ANIMAL_ID, DATETIME FROM ANIMAL_INS) INS
    INNER JOIN
    (SELECT ANIMAL_ID, NAME, DATETIME FROM ANIMAL_OUTS) OUTS
    ON INS.ANIMAL_ID = OUTS.ANIMAL_ID
ORDER BY DATEDIFF(INS.DATETIME, OUTS.DATETIME)
LIMIT 2;
