# 문제 링크 : https://school.programmers.co.kr/learn/courses/30/lessons/301647

SELECT
    CHILD.ID AS 'ID',
    CHILD.GENOTYPE AS 'GENOTYPE',
    PARENT.GENOTYPE AS 'PARENT_GENOTYPE'
FROM
    (SELECT ID, GENOTYPE FROM ECOLI_DATA) PARENT
    INNER JOIN (SELECT ID, PARENT_ID, GENOTYPE FROM ECOLI_DATA) CHILD
    ON CHILD.PARENT_ID = PARENT.ID AND (CHILD.GENOTYPE & PARENT.GENOTYPE) = PARENT.GENOTYPE
ORDER BY 1;
